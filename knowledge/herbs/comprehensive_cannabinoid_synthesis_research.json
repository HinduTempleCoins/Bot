  "part_2_biosynthetic_pathway": {
    "title": "The Biosynthetic Pathway — Every Cannabinoid Starts Here",

    "universal_precursor": {
      "name": "Cannabigerolic Acid (CBGA)",
      "description": "All pentyl cannabinoids derive from CBGA through enzymatic conversion",
      "formation": "Olivetolic Acid + Geranyl Pyrophosphate (GPP) → CBGA (via Aromatic Prenyltransferase)"
    },

    "pathway_diagram": {
      "step_1": "Olivetolic Acid + Geranyl Pyrophosphate (GPP)",
      "enzyme_1": "Aromatic Prenyltransferase",
      "step_2": "CBGA (Cannabigerolic Acid)",
      "branches": {
        "thca_pathway": {
          "enzyme": "THCA Synthase",
          "product": "THCA",
          "decarboxylation": "THC (via heat)"
        },
        "cbda_pathway": {
          "enzyme": "CBDA Synthase",
          "product": "CBDA",
          "decarboxylation": "CBD (via heat)"
        },
        "cbca_pathway": {
          "enzyme": "CBCA Synthase",
          "product": "CBCA",
          "decarboxylation": "CBC (via heat)"
        }
      }
    },

    "key_synthases": {
      "thca_synthase": "Converts CBGA → THCA (cyclizes to form pyran ring)",
      "cbda_synthase": "Converts CBGA → CBDA (keeps ring open)",
      "cbca_synthase": "Converts CBGA → CBCA (forms chromene structure)"
    },

    "non_enzymatic_transformations": {
      "CBN": "Oxidation of THC (exposure to air, light, heat)",
      "CBL": "Photochemical conversion of CBC",
      "Δ8-THC": "Isomerization of Δ9-THC under acidic conditions",
      "CBE": "Degradation product"
    }
  },

  "part_3_thcp_synthesis": {
    "title": "Synthesis of THCp — The Process Revealed",

    "natural_occurrence": {
      "concentration_range": "0.0023% to 0.0136%",
      "implication": "Far too low for commercial extraction — lab synthesis is the only viable production method"
    },

    "synthesis_method_citti_2019": {
      "source": "2019 Citti et al. Discovery Paper",
      "starting_material": {
        "name": "5-heptylbenzene-1,3-diol (sphaerophorol)",
        "description": "A resorcinol with 7-carbon side chain"
      },

      "step_1_cbdp_formation": {
        "reagents": [
          "5-heptylbenzene-1,3-diol (1.1 eq)",
          "p-toluenesulfonic acid (pTSA, 0.1 eq)",
          "Dichloromethane (DCM) as solvent"
        ],
        "conditions": "Room temperature for 90 minutes",
        "product": "CBDP (cannabidiphorol)"
      },

      "step_2_delta8_thcp": {
        "reagents": ["pTSA (0.1 eq)", "DCM"],
        "conditions": "Room temperature for 48 hours",
        "mechanism": "Cyclization occurs",
        "product": "Δ8-THCp"
      },

      "step_3_delta9_thcp": {
        "hydrochlorination": {
          "catalyst": "ZnCl₂ (0.5 eq)",
          "reagent": "4N HCl in dioxane",
          "solvent": "Dry DCM under argon atmosphere",
          "conditions": "0°C to room temperature for 2 hours",
          "product": "(-)-trans-HCl-THCp intermediate"
        },
        "selective_elimination": {
          "reagent": "Potassium t-amylate (2.5 eq)",
          "solvent": "Dry toluene under argon",
          "conditions": "-15°C for 1 hour",
          "final_yield": "87% Δ9-THCp"
        }
      }
    },

    "alternative_cbd_to_thcp": {
      "description": "The industry primarily uses this pathway",
      "steps": [
        "Start with CBD isolate from legal hemp",
        "Alkyl chain extension (KEY STEP) — convert 5-carbon pentyl chain to 7-carbon heptyl chain",
        "Proceed with standard acid-catalyzed cyclization"
      ],
      "complexity_note": "More complex than simple CBD→THC isomerization because it requires modifying the side chain length, not just ring cyclization"
    }
  },

